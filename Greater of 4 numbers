//THIS CAN WORK UPTO n Numbers without use of nested if

#include <iostream>
#include <cstdio>
using namespace std;

int max_of_four(int a, int b, int c, int d)
{ int lar=a;
    if(a>lar){
        lar=a;
    }
 if(b>lar){
     lar=b;
 }
 if(c>lar){
     lar=c;
 }
 if(d>lar){
     lar=d;
 }
 return lar;
}
int main() {
    int a, b, c, d;
    scanf("%d %d %d %d", &a, &b, &c, &d);
    int ans = max_of_four(a, b, c, d);
    printf("%d", ans);
    
    return 0;
}

